<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.refresh.smart.cn">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".ui.Classical2Activity"></activity>
        <activity android:name=".ui.ClassicalActivity"></activity>

        <activity android:name=".ui.WaveSwipeActivity"></activity>

        <activity android:name=".ui.WaterDropActivity"></activity>

        <activity android:name=".ui.MeituanActivity"></activity>


        <activity android:name=".ui.PullMoreAndRefreshActivity"></activity>
        <activity android:name=".baseUse.RecycleviewBasicActivity"></activity>

    </application>

</manifest>